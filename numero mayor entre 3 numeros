#include <iostream>
#include "conio.h"

using namespace std;

int main()
{
  int numero1, numero2, numero3;
  cout<<"Ingrese el numero: "<<endl,
  cin>>numero1;
  cout<<"Ingrese el numero: "<<endl,
  cin>>numero2;
  cout<<"Ingrese el numero: "<<endl,
  cin>>numero3;
  if(numero1>=numero2)
    if (numero1>=numero3)
        cout<<"El mayor es: "<<numero1<<endl;
    else
        cout<<"El mayor es: "<<numero3<<endl;
        
   else
    if (numero2>=numero3)
        cout<<"El mayor es: "<<numero2<<endl;
    else
        cout<<"El mayor es: "<<numero3<<endl,
    getch();
    return 0;
}
